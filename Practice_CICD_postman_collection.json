						"exec": [
							"var jsonResp = pm.response.json();",
							"postman.setEnvironmentVariable(\"token\", jsonResp[\"access_token\"]);",
							"",
							"pm.test(\"Json contains valid Message\", function() {",
							"    pm.expect(jsonResp.token_type).to.contain(\"bearer\")",
